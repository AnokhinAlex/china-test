@import "reset";
@import "header";
.button
{
    background-color:  #a2d965;
    max-width: 256px;
    width: 100%;
    height: 56px;
    line-height: 56px;
    text-align: center;
    display: inline-block;
    color:  #424242;
    font-size: 24px;
    font-weight: 300;
    text-transform: uppercase;
    vertical-align: middle;
    border: none;
    letter-spacing: 5px;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -ms-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
    &:hover
    {
        background-color:  #85ac59;
    }
    &:focus
    {
        background-color:  #82f700;
    }
}
.banner
{
    background: url(../images/bg.png)no-repeat;
    background-size: cover;
    
    .banner-content
    {
        min-height: 45.84vw;
        display: -webkit-flex;
        display: -moz-flex;
        display: -ms-flex;
        display: -o-flex;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        
        h1.title
        {
            color:  #fff;
            font-size: 72px;
            font-weight: 400;
            text-align: center;
            max-width: 745px;
            text-transform: uppercase;
        }
        p
        {
            color:  #fff;
            font-size: 18px;
            font-weight: 400;
            line-height: 24px;
            text-align: center;
            max-width: 745px;
            font-family: 'TitilliumWeb';
        }
        form
        {
            max-width: 745px;
            width: 100%;
            text-align: center;
            margin-top: 60px;
        }
        input
        {
            &[type="email"]
            {
                border:1px solid #424242;
                background-color:  #fff;
                width: 410px;
                height: 56px;
                display: inline-block;
                font-family: 'TitilliumWeb';
                font-size: 18px;
                padding: 0 15px;
                vertical-align: middle;
                margin-right: 5px;
            }
            &[type="submit"]
            {
                display: inline-block;
                vertical-align: middle;
                margin-left: 5px;  
            }
        }
    }
}
.items
{
    padding:65px 0;
    display: -webkit-flex;
    display: -moz-flex;
    display: -ms-flex;
    display: -o-flex;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    .item
    {
        width: calc(100% / 3);
        float: left;
        text-align: center;
        
        h3
        {
            margin: 40px 0 20px;
            color:  #424242;
            font-size: 36px;
            
        }
        p
        {
            font-family: "TitilliumWeb";
            color:  #424242;
            font-size: 20px;
            font-weight: 400;
            max-width: 240px;
            display: table;
            margin: 0 auto;
        }
        i
        {
            &:before
            {
                font-size: 97px;    
            }
        }
    }
}
.descriptions
{
    display: -webkit-flex;
    display: -moz-flex;
    display: -ms-flex;
    display: -o-flex;
    display: flex;
    flex-direction: row;
}
.description
{
    width: calc(100% / 2 - 15px);
    float: left;
    padding-top: 60px;
    padding-bottom: 60px;
    &:nth-of-type(2)
    {
        margin-left: 30px;
    }
    p
    {
        max-width: 435px;
        width: 100%;
        display: table;
        margin: 0 auto;
        font-family: 'TitilliumWeb';
        color:  #424242;
        font-size: 20px;
        font-weight: 400;
    }
}
.slider
{
    .item
    {
        text-align: center;
        img
        {
            max-width: 420px; 
            display: table;
            margin: 0 auto  40px;
        }
        h2.title
        {
            color:  #424242;
            font-size: 48px;
            font-weight: 400;
            margin-bottom: 50px;
            &:after
            {
                display: none;
            }
        }
        p
        {
            font-family: 'TitilliumWeb';
            color:  #424242;
            font-size: 20px;
            font-weight: 400;
            max-width: 790px;
            width: 100%;
            display: table;
            margin: 0 auto 55px;
        }
    }
}
h2.title
{
    color:  #424242;
    font-size: 36px;
    line-height: 1.2;
    text-align: center;
    display: table;
    margin: 0 auto;
    position: relative;
    
    &:after
    {
        content: '';
        height: 2px;
        background: #424242;
        width: 100%;
        position: absolute;
        bottom: -5px;
        left: 0;
        right: 0;
    }
}
.who-we-are
{
    padding: 65px 0;
    
    h3.title
    {
        color: #424242;
        font-size: 36px;
        line-height: 1.2;
        text-align: center;
        display: table;
        margin: 0px auto;
        position: relative;
        padding: 20px 0;
        width: 100%;
        &:before
        {
            bottom: inherit;
            top: -10px;
        }
        &:before,
        &:after
        {
            content: '';
            height: 2px;
            background: #424242;
            width: 100%;
            position: absolute;
            bottom: -5px;
            left: 0;
            right: 0;
        }
    }
}
.advantages
{
    padding-bottom: 50px;
    a.button
    {
        display: table;
        margin: 0 auto;
        background: #707070;
        color: #fff;
        &:hover
        {
            background-color:  #424242;
        }
        &:focus
        {
            background-color:  #010101;
        }
    }
}
.order-process
{
    background: url(../images/bg-2.png)no-repeat;
    background-size: cover;
    padding: 95px 0;
    color: #fff;
    
    h2.title
    {
        color: #fff;
        
        &:after
        {
            background: #fff;
        }
    }
    .items
    {
        padding-bottom: 0;
        position: relative;
        &:before
        {
            content: '';
            width: 80%;
            left: 0;
            right: 0;
            bottom: 0;
            position: absolute;
            height: 2px;
            background: #fff;
            display: table;
            margin: 0 auto;
        }
    }
    a.button
    {
        display: table;
        margin: 0 auto;
    }
    .descriptions
    {
        width: 80%;
        display: table;
        margin: 0 auto;
    }
    .description
    {
        &:first-of-type
        {
            padding-right: 50px;
        }
        &:last-of-type
        {
            padding-left: 50px;
        }
        p
        {
            color: #fff;
        }
    }
    .item
    {
        margin-bottom: 60px;
        &:first-of-type
        {
            i
            {
                &:before
                {
                    font-size: 114px;
                }
            }
            p
            {
                margin-top: -2px;
            }
        }
        p
        {
            color: #fff;
            margin-top: 15px;
        }
    }
}
.our-production
{
    padding: 95px 0;
    
    h2.title
    {
        margin-bottom: 80px;
    }
}
.top-sales
{
    .item
    {
        width: calc(100% / 3 - 82px);
        margin-right: 123px !important;
        &:nth-of-type(3n)
        {
            margin-right: 0 !important;
        }
    }
}
.top-sales,
.catalog
{
    padding-bottom: 95px;
    .items
    {
        padding-top: 0;
    }
    .item
    {
        text-align: left;
        margin:60px 0 0px;
        border: 1px solid transparent;
        -webkit-transition: all 0.5s ease;
        -moz-transition: all 0.5s ease;
        -ms-transition: all 0.5s ease;
        -o-transition: all 0.5s ease;
        transition: all 0.5s ease;
        &:hover
        {
            border: 1px solid #424242;
            box-shadow: 1px 2px 63.24px 4.76px rgba(0, 0, 0, 0.15);
        }
        .description-item
        {
            max-width: 296px;
            width: 100%;
            display: table;
            margin: 0 auto;
            
            img
            {
                width: 100%;
            }
        }
        h3
        {
            color:  #424242;
            font-size: 24px;
            font-weight: 400;
            margin: 20px 0 10px;
            padding: 0 10px;
        } 
        a
        {
            display: table;
            margin: 0 auto;
            
        }
        p
        {
            font-family: "TitilliumWeb";
            color:  #424242;
            font-size: 16px;
            font-weight: 400;
            line-height: 24px;
            margin: 0;
            padding: 0 10px 20px;
        }
    }
    a.button
    {
        margin: 0 auto;
        display: table;
        background: #0b77df;
        color: #fff;
        
        &:focus
        {
            background-color:  #006cfe;
        }
        &:hover
        {
            background-color:  #0056a9;
        }
    }
}
#map
{
    width: calc(100% + 30px);
    margin-left: -15px;
    margin-right: -15px;
    height: 990px;
}
.map
{
    position: relative;
    
    h2.title
    {
        color: #fff;
        position: absolute;
        top: 45px;
        left: 0;
        right: 0;
        display: table;
        margin: 0px auto;
        z-index: 100;
        text-shadow: 2px 2px 3px rgba(0, 0, 0, 0.4);
        &:after
        {
            background: #fff;
            box-shadow: 1px 1px 8px 1px rgba(0, 0, 0, 0.4);
        }
    }
}
.about-returns
{
    background-color:  #00a850;
    padding: 95px;
    h2.title
    {
        color: #fff;
        margin-bottom: 65px;
        &:after
        {
            background: #fff;
        }
    }
    p
    {
        color: #fff;
        font-size: 20px;
        font-weight: 400;
        font-family: "TitilliumWeb";
        text-align: center;
        max-width: 885px;
        display: table;
        margin: 0 auto;
    }
}
.clients-about-us
{
    padding: 95px 0;
    h2.title
    {
        margin-bottom: 75px;
    }
    .video
    {
        width: calc(100% / 2 - 7.5px);
        float: left;
        &:nth-of-type(2n)
        {
            margin-left: 15px;
        }
        iframe
        {
            width: 100%;
            height: 310px;
        }
        p
        {
            color:  #424242;
            font-size: 30px;
            font-weight: 400;
        }
    }
}
.contact-form
{
    background: #424242;
    padding: 95px 0;
    
    h2.title
    {
        color: #fff;
        margin-bottom: 80px;
        &:after
        {
            background-color:  #a2d965;
        }
    }
    .left-block,
    .right-block
    {
        width: calc(100% / 2 - 50px); 
        float: left;
        label
        {
            color:  #fff;
            font-size: 24px;
            font-weight: 400;
        }
        input
        {
            &[type="text"],
            &[type="tel"],
            &[type="email"]
            {
                padding: 0 15px;
                height: 40px;
                width: 100%;
                margin-bottom: 22.8px;
            }
            &[type="tel"]
            {
                margin-bottom: 10px;
            }
        }
        p
        {
            font-family: 'TitilliumWeb';
            color:  #ffffff;
            font-size: 14px;
            font-weight: 400;
        }
    }
    .button
    {
        display: table;
        margin: 0 auto;
        clear: both;
    }
    .right-block
    {
        margin-left: 100px;
        textarea
        {
            width: 100%;
            height: 315px;
            resize: none;
        }
    }
}
.shipping-and-return
{
    background: #00BBF0 url(../images/bg-3.png)no-repeat bottom center;
    padding: 65px 0 330px;
    h4.title
    {
        color:  #424242;
        font-size: 30px;
        text-align: center;
        font-weight: 400;
        margin-top: 70px;
    }
    .table
    {
        & + .descriptions
        {
            border-top: 2px solid #fff; 
            clear: both;
            .description
            {
                padding-top: 30px;
               
            }
            p
            {
                color: #fff;
                max-width: 100%;
            }
        }
        .tr
        {
            overflow: hidden;
            .th,
            .td
            {
                width: calc(100% / 4);
                float: left;
                line-height: 95px;
                padding-left: 30px;
            }
            &:first-of-type
            {
                background-color:  #424242;
                P
                {
                    color: #fff;
                    font-size: 43px;
                    font-weight: 500;
                    text-transform: uppercase;
                }
            }
            .td
            {
                p
                {
                    font-family: 'TitilliumWeb';
                    color:  #424242;
                    font-size: 28px;
                    font-weight: 300;
                }
            }
        }
    }
    .descriptions
    {
        p
        {
            max-width: 100%;
        }
        .description
        {
            &:last-of-type
            {
                padding-left: 20px;
            }
            &:first-of-type
            {
                padding-right: 20px;
            }
        }
    }
}
.cat
{
    margin-top: 50px;
    overflow: hidden;
    position: relative;
    padding-bottom: 15px;
    h2.title
    {
        margin-bottom: 60px;
        text-transform: uppercase;
        padding-top: 10px;
    }
    &:before
    {
        height: 2px;
        width: 100%;
        left: 0;
        right: 0;
        position: absolute;
        content: '';
        background: #424242;
        bottom: 0px;
    }
    li
    {
        width: calc(100% / 10);
        float: left;
        -webkit-transition: all 0.5s ease;
        -moz-transition: all 0.5s ease;
        -ms-transition: all 0.5s ease;
        -o-transition: all 0.5s ease;
        transition: all 0.5s ease;
        &.active,
        &:hover
        {
            background: #0d9efd;
            a
            {
                color: #fff;
                text-decoration: underline;
            }

        }
        &:first-of-type
        {
            a
            {
                font-size: 24px;
                padding: 25.6px 0;
            }

        }
        a
        {
            color:  #555555;
            font-size: 18px;
            font-weight: 400;
            text-transform: uppercase;
            display: table;
            margin: 0px auto;
            padding: 7.6px 0px;
        }
    }
}
.catalog
{
    padding:40px 0  0;
    ul
    {
        li
        {
            display: inline-block;
            &:last-of-type
            {
                &:after
                {
                    display: none;
                }
            }
            &:after
            {
                content: '/';
                font-family: 'TitilliumWeb';
                color:  #424242;
                font-size: 24px;
                display: inline-block;
                margin: 0 5px;
            }
            a
            {
                font-family: 'TitilliumWeb';
                color:  #424242;
                font-size: 24px;
                font-weight: 600;
            }
        }
    }
    .items
    {
        padding-bottom: 0;
        .item
        {
            width: calc(100% / 4 - 17px);
            margin-right: 22px;
            &:nth-child(-n+4)
            {
                margin-top: 20px;    
            }
            &:nth-of-type(4n)
            {
                margin-right: 0;
            }
        }
    }
    &.card
    {
        padding-top: 25px;
        h3.title
        {
            color:  #424242;
            font-size: 36px;
            font-weight: 400;
            margin: 35px 0 55px;
        }
        .left-block
        {
            width: calc(100% / 2 - 15px);
            float: left;
            
            .item-slider 
            {
                img
                {
                    width: 100%;
                }
            }
        }
        .right-block
        {
            width: calc(100% / 2 - 15px);
            float: left;
            margin-left: 30px;
            
            li
            {
                &:after
                {
                    display: none;
                    
                }
            }
            .nav-tabs
            {
                overflow: hidden;
                border-bottom: 2px solid #424242;
                
                li
                {
                    width: calc(100% / 2);
                    float: left;
                    text-align: center;
                    padding: 10px 0;
                    -webkit-transition: all 0.5s ease;
                    -moz-transition: all 0.5s ease;
                    -ms-transition: all 0.5s ease;
                    -o-transition: all 0.5s ease;
                    transition: all 0.5s ease;
                    &.active,
                    &:hover
                    {
                        background-color:  #424242;
                        a
                        {
                            color: #fff;
                        }
                    }
                    a
                    {
                        font-size: 30px;
                        font-weight: 400;
                    }
                }
            }
            .tab-content
            {
                background: #f6f6f6;
                padding: 40px 35px;
                height: 360px;
                
                h5
                {
                    font-family: 'TitilliumWeb';
                    color:  #424242;
                    font-size: 18px;
                    font-weight: 700;
                    line-height: 24px;
                }
                p
                {
                    font-family: 'TitilliumWeb';
                    margin-bottom: 20px;
                    color:  #424242;
                    font-size: 18px;
                    font-weight: 400;
                    line-height: 24px;
                }
                ul
                {
                    padding-left: 20px;
                    li
                    {
                        font-family: 'TitilliumWeb';
                        display: block;
                        color:  #424242;
                        font-size: 18px;
                        font-weight: 400;
                        line-height: 24px;
                    }
                }
            }
            a.button
            {
                background-color: #a2d965;;
                color: #424242;
                margin: 35px 0 65px;
                
                &:hover
                {
                    background-color:  #85ac59;
                }
                &:focus
                {
                    background-color:  #82f700;
                }
            }
        }
        .slider-product
        {
            .slick-arrow
            {
                &.slick-next
                {
                    right: 20px;
                    
                    &:before
                    {
                        content: '\0052';
                    }
                    &:after
                    {
                        right: -4px;
                    }
                }
                &.slick-prev
                {
                    left: 20px;
                    &:before
                    {
                        content: '\0051';
                    }
                    &:after
                    {
                        left: -4px;
                    }
                }
                &.slick-prev,
                &.slick-next
                {
                    top: 50%;
                    -webkit-transform: translateY(-50%);
                    -moz-transform: translateY(-50%);
                    -ms-transform: translateY(-50%);
                    -o-transform: translateY(-50%);
                    transform: translateY(-50%);
                    
                    &:after
                    {
                        background: #fff !important;
                        border-radius: 50%;
                        text-shadow: 0 0 3px rgba(255,255,255,1);
                        content: '';
                        position: absolute;
                        top: 50%;
                        -webkit-transform: translateY(-50%);
                        -moz-transform: translateY(-50%);
                        -ms-transform: translateY(-50%);
                        -o-transform: translateY(-50%);
                        transform: translateY(-50%);
                        width: 64px;
                        height: 64px;
                        z-index: -1;
                        top: 29px;
                    }
                }
            }
            .item-slider
            {
                position: relative;
                cursor: pointer;
            }
        }
        .slider-product-nav
        {
            clear: both;
            width: calc(100%  - 110px);
            margin-left: 55px;
            margin:0 55px;
            
            .slick-arrow
            {
                &.slick-next
                {
                    right: -50px;
                    
                    &:before
                    {
                        content: '\0052';
                    }
                }
                &.slick-prev
                {
                    left: -50px;
                    &:before
                    {
                        content: '\0051';
                    }
                }
                &.slick-prev,
                &.slick-next
                {
                    top: 50%;
                    -webkit-transform: translateY(-50%);
                    -moz-transform: translateY(-50%);
                    -ms-transform: translateY(-50%);
                    -o-transform: translateY(-50%);
                    transform: translateY(-50%);
                }
            }
            .item-slider
            {
                padding:0 10px;
                position: relative;
                cursor: pointer;
            }
            img
            {
                width: 100%;
                
            }
            [data-fancybox="gallery"]
            {
                &:before
                {
                    right: 20px;
                }
            }
        }
    }
}
[data-fancybox="gallery"]
{
    &:before
    {
        content: '\0043';
        font-family: 'icon';
        font-size: 38px;
        position: absolute;
        right: 10px;
        top: 5px;
        color: #fff;
        z-index: 10;
    }
}
.enquiry 
{
    .modal-content
    {
        background-color:  #0b77df;
        padding: 75px 50px 30px;
        input
        {
            &[type="text"],
            &[type="email"]
            {
                height: 40px;
                width: 100%;
                margin-bottom: 40px;
                
            }
        }
        label
        {
            color:  #ffffff;
            font-size: 24px;
            font-weight: 400;
        }
        textarea
        {
            resize: none;
            width: 100%;
            height: 285px;
        }
        p
        {
            font-family: 'TitilliumWeb';
            color:  #ffffff;
            font-size: 14px;
            font-weight: 400;
            margin: 5px 0 20px;
        }
        .button
        {
            margin: 0 auto;
            display: table;
        }
        i
        {
            position: absolute;
            top: 20px;
            right: 20px;
            cursor: pointer;
            &:before
            {
                font-size: 43px;
                color: #fff;
            }
        }
        
    }
}
.blog
{
    h2.title
    {
        margin: 70px auto 50px;
        text-transform: uppercase;
    }
    .blog-banner
    {
        background-size: cover;  
        min-height: 500px;
    }
    .banner-content
    {
        padding: 90px 0 100px;
        h1.title
        {
            color:  #ffffff;
            font-size: 72px;
            font-weight: 400;
            text-transform: uppercase;
            letter-spacing: 5px;
            text-align: center;
            max-width: 730px;
            width: 100%;
            display: table;
            margin: 0 auto;
        }
        p
        {
            font-family: 'TitilliumWeb';
            color:  #ffffff;
            font-size: 18px;
            font-weight: 400;
            line-height: 24px;
            max-width: 750px;
            width: 100%;
            display: table;
            margin: 0 auto;
            text-align: center;
        }
    }
    
}
.blog-items
{
    padding-top: 100px;
    .blog-item
    {
        border: 1px solid  #424242;
        background-color:  #f5f5f5;
        margin-bottom: 25px;
    }
    .left-block
    {
        width: 440px;
        display: inline-block;
        vertical-align: middle;
        img
        {
            width: 100%;
            margin-bottom: -5px;
        }
    }
    .right-block
    {
        width: calc(100% - 445px);
        display: inline-block;
        vertical-align: middle;
        padding: 60px 70px 40px;
        
        h3.title
        {
            
            font-size: 36px;
            font-weight: 400;
            line-height: 35px;
            text-transform: uppercase; 
        }
        a
        { 
            display: block;
            -webkit-transition: all 0.5s ease;
            -moz-transition: all 0.5s ease;
            -ms-transition: all 0.5s ease;
            -o-transition: all 0.5s ease;
            transition: all 0.5s ease;
            h3.title
            {
                color: #424242;
            }
            &:visited
            { 
                h3.title
                {
                    color:  #3b8be2; 
                }
            }
            &:hover
            {
                h3.title
                {
                    color:  #3b8be2;
                    text-decoration: underline;  
                } 
            }
        }
        
        p
        {
            font-family: TitilliumWeb;
            color:  #555555;
            font-size: 16px;
            font-weight: 300;
            line-height: 22px;
            margin: 10px 0;
        }
        span
        {
            margin-top: 15px;
            color:  #959595;
            font-size: 18px;
            font-weight: 300;
            text-transform: uppercase;
    
        }
    }
}
.sline
{
